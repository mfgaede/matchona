# gitignore lists files you want Git to NOT track!
# Ignore metadata generated by Jekyll
_site/
.sass-cache/
.jekyll-cache/
.jekyll-metadata

# Ignore folders generated by Bundler
.bundle/
vendor/

# Ignore mac files 
.DS_Store

# temporarily ignore gemfile.lock
Gemfile.lock

# Large media objects
/objects/
/assets/audio/
/assets/video/
/assets/large-files/

# IDE and editor specific files
.vscode/
.idea/
*.sublime-project
*.sublime-workspace

# Environment variables and secrets
.env
.env.*
*.pem
*.key

# Logs and temporary files
*.log
log/
tmp/
.tmp/

# Node modules (if you use any JavaScript dependencies)
node_modules/

# Ruby environment manager files
.ruby-version
.ruby-gemset

# System files
Thumbs.db
ehthumbs.db